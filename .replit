modules = ["nodejs-20", "web", "python-3.12", "postgresql-16"]
[nix]
channel = "stable-25_05"
packages = ["unzip"]

[agent]
expertMode = true
integrations = ["javascript_openai:1.0.0", "javascript_log_in_with_replit:2.0.0", "javascript_database:1.0.0"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Dento Health Care"

[[workflows.workflow]]
name = "Dento Health Care"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd Dento-HealthCarezip/Dento-HealthCare && npm run dev"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80
